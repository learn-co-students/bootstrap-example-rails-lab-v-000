<!DOCTYPE html>
<html>
<head>
  <title>Application Layout</title>
  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
</head>
<body>
  <div class="container">
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class=" nav navbar-nav">
          <li class="nav-item active">
            <a class="nav-link" href="#">Home</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">About</a>
          </li>
          <li class= "nav-item"> 
            <a class="nav-link" href="#">Contact</a>
          </li>
        </ul>
      </div>
    </nav>
    <div class="row">
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-primary">1</p>
      </div>
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-primary">2</p>
      </div>
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-primary">3</p>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-primary">4</p>
      </div>
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-primary">5</p>
      </div>
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-primary">6</p>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-primary">7</p>
      </div>
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-primary">8</p>
      </div>
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-primary">9</p>
      </div>
    </div>
  </div>
<%= yield %>
<%= javascript_include_tag "application" %>
</body>
</html>
