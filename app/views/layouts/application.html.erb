      </div>
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-outline-success">Success</button></p>
      </div>
    </div>
    <!-- row -->
  </div>
  <!-- container   -->
<%= yield %>
<%= javascript_include_tag "application" %>
</body>
</html>
