<!DOCTYPE html>
<html>
<head>
  <title>Application Layout</title>
  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
</head>
<body>
  <div class="container">
    <ul class="nav navbar-nav">
      <li role="presentation" class="active"><a href="#">Home</a></li>
      <li role="presentation"><a href="#">About</a></li>
      <li role="presentation"><a href="#">Contact</a></li>
    </ul>

    <div class="row">
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-default">Default</button>1</p>
      </div>
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-primary">Primary</button>2</p>
      </div>
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-success">Success</button>3</p>
      </div>
    </div>

    <div class="row">
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-info">Info</button>4</p>
      </div>
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-warning">Warning</button>5</p>
      </div>
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-danger">Danger</button>6</p>
      </div>
    </div>

    <div class="row">
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-link">link</button>7</p>
      </div>
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-primary btn-lg">Large</button>8</p>
      </div>
      <div class="col-lg-4">
        <p><button type="button" class="btn btn-primary btn-sm">Small</button>9</p>
      </div>
    </div>
  </div>

<%= yield %>
<%= javascript_include_tag "application" %>
</body>
</html>
